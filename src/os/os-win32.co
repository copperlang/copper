struct DVTARGETDEVICE

	attr tdSize	: DWORD
	attr tdDriverNameOffset	: WORD
	attr tdDeviceNameOffset	: WORD
	attr tdPortNameOffset	: WORD
	attr tdExtDevmodeOffset	: WORD
	attr tdData	: [1] BYTE

struct FORMATETC

	attr cfFormat	: CLIPFORMAT
	attr ptd	: DVTARGETDEVICE
	attr dwAspect	: DWORD
	attr lindex	: LONG
	attr tymed	: DWORD

struct STGMEDIUM

	attr tymed	: DWORD
	attr anonymousUnion	: HANDLE
	attr pUnkForRelease	: LPUNKNOWN

struct IEnumFORMATETCVtbl

	attr QueryInterface	: & (THIS, REFIID, *[] PVOID)	-> (HRESULT)
	attr AddRef	: & (THIS)	-> (ULONG)
	attr Release	: & (THIS)	-> (ULONG)
	attr Next	: & (THIS, ULONG, LPFORMATETC, ULONG)	-> (HRESULT)
	attr Skip	: & (THIS, ULONG)	-> (HRESULT)
	attr Reset	: & (THIS)	-> (HRESULT)
	attr Clone	: & (THIS, *IEnumFORMATETC)	-> (HRESULT)

struct IEnumFORMATETC

	attr lpVtbl	: *IEnumFORMATETCVtbl

struct IDataObjectVtbl

	attr QueryInterface	: & (THIS, REFIID, *[] PVOID)	-> (HRESULT)
	attr AddRef	: & (THIS)	-> (ULONG)
	attr Release	: & (THIS)	-> (ULONG)
	attr GetData	: & (THIS, LPFORMATETC, LPSTGMEDIUM)	-> (HRESULT)
	attr GetDataHere	: & (THIS, LPFORMATETC, LPSTGMEDIUM)	-> (HRESULT)
	attr QueryGetData	: & (THIS, LPFORMATETC)	-> (HRESULT)
	attr GetCanonicalFormatEtc	: & (THIS, LPFORMATETC, LPFORMATETC)	-> (HRESULT)
	attr SetData	: & (THIS, LPFORMATETC, LPSTGMEDIUM, BOOL)	-> (HRESULT)
	attr EnumFormatEtc	: & (THIS, DWORD, *[] *IEnumFORMATETC)	-> (HRESULT)

struct IDataObject

	attr lpVtbl : *IDataObjectVtbl

struct IDropTargetVtbl

	attr QueryInterface	: & (THIS, REFIID, *[] PVOID)	-> (HRESULT)
	attr AddRef	: & (THIS)	-> (ULONG)
	attr Release	: & (THIS)	-> (ULONG)
	attr DragEnter	: & (THIS, LPDATAOBJECT, DWORD, LONG, LONG, *[] DWORD)	-> (HRESULT)
	attr DragOver	: & (THIS, DWORD, LONG, LONG, *[] DWORD)	-> (HRESULT)
	attr DragLeave	: & (THIS)	-> (HRESULT)
	attr Drop	: & (THIS, LPDATAOBJECT, DWORD, LONG, LONG, *[] DWORD)	-> (HRESULT)

struct IDropTarget

	attr lpVtbl : *IDropTargetVtbl

